{
  "version": 2,
  "builds": [
    {
      "src": "app.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/",
      "dest": "public"
    },
    {
    "src": "/apisetting/(.*)",
    "dest": "app.js"
  }
  
  ]
}